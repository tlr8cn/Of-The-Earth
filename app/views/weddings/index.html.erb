<div class="container">
  <div class="row">
  	<div class="col-sm-12 logo-tabs">
  	  <div class="row logo-container">
  	  	<div class="col-sm-12">
			<%= link_to "/home/index" do %>
				<%= image_tag "/assets/logo-3.jpg", alt: "Logo", class: "logo-image" %>
			<% end %>
  	  	</div>
  	  </div>
  	  <div class="row tabs-container">
  	  	<nav>
	  	    <div class="col-sm-4 primary-tab about-me">

	  	    		<ul class="cf">

			  	    	<li>
			  	    		<div class="row">

			  	    			<div class="col-sm-2 about-me-flowers-cont-left">
	  	    						<img class="about-me-flowers" src="/assets/flower.gif" />
				  	    		</div>
				  	    		<div class="col-sm-8 about-me-label-cont">

				  	    			<%= link_to "ABOUT ME \u25BC", "javascript:void(0)" , class: "about-me-list-item about-me-mobile tab-primary-link dropdown" %>

				  	    			<%= link_to "ABOUT ME", "javascript:void(0)" , class: "about-me-list-item about-me-desktop tab-primary-link dropdown" %>


				  	    		</div>
				  	    			<div class="col-sm-2 about-me-flowers-cont-right">


	  	
				  	    			</div>
				  	    	</div>

				  	    	<ul class="sub-items about-me-sub-items" dropped="false">
				                <li class="about-me-1">
				  	    			<%= link_to "Who am I?", {controller: "about_me"}, class: "about-me-item sub-item" %>
				                <li class="about-me-3">
				                	<%= link_to "Client Testimonials", {controller: "client_testimonials"}, class: "about-me-item sub-item" %>
				                		
				                </li>
				            </ul>
			            </li>
			        </ul>
		        
	  	    </div>
	  	    <div class="col-sm-4 primary-tab my-work">
	  	    	<ul class="cf">
	  	    		<li>

	  	    				<div class="row">

			  	    			<div class="col-sm-2 my-work-flowers-cont-left">
	  	    						<img class="my-work-flowers" src="/assets/flower.gif" />
				  	    		</div>
				  	    		<div class="col-sm-8 my-work-label-cont">

	  	    					<%= link_to "MY WORK", "javascript:void(0)", class: "tab-primary-link dropdown my-work-list-item my-work-desktop" %>

	  	    					<%= link_to "MY WORK \u25BC", "javascript:void(0)", class: "tab-primary-link dropdown my-work-list-item my-work-mobile" %>



				  	    		</div>
				  	    			<div class="col-sm-2 my-work-flowers-cont-right">

	  	    						<img class="my-work-flowers" src="/assets/flower.gif" />
	  	
				  	    		</div>
				  	    	</div>



	  	    			<ul class="sub-items my-work-sub-items" dropped="false">
				  	    	<li><%= link_to "Weddings", {controller: "weddings"}, class: "my-work-item sub-item" %></li>
				            <li><%= link_to "Beautiful Moments", {controller: "beautiful_moments"}, class: "my-work-item sub-item" %></li>
	  	    			</ul>
	  	    		</li>
	  	    	</ul>
	  	    </div>

	  	    <div class="col-sm-4 primary-tab book-online">
	  	    	<ul class="cf">
	  	    		<li>
	  	    				<div class="row">

			  	    			<div class="col-sm-2 book-online-flowers-cont-left">

				  	    		</div>
				  	    		<div class="col-sm-8 book-online-label-cont">

	  	    					<%= link_to "BOOK ONLINE", { controller: "book_online" }, class: "tab-primary-link dropdown" %>

				  	    		</div>
				  	    			<div class="col-sm-2 book-online-flowers-cont-right">

	  	    						<img class="book-online-flowers" src="/assets/flower.gif" />
	  	
				  	    		</div>
				  	    	</div>


	  	    		</li>
	  	    	</ul>
	  	    </div>

  	    </nav>
  	  </div>
  	</div>
  </div>
  <div class="row">
  	<div class="col-sm-12 carousel-container">

  			<% (1..51).each do |i| %>
	  			<div class="row gallery-row">
		  			<% (1..3).each do |j| %>
		  				<div class="col-sm-4">
		  					<%= image_tag image_path("weddings/#{@x}.jpg"), class: "gallery-image", id: "galImg" %>
		  				</div>
		  				<% @x += 1 %>
		  			<% end %>
		  		</div>
	  		<% end %>

	  		<div id="myModal" class="modal">
			  <span class="close">&times;</span>
			  <img class="modal-content" id="img01">
			  
			</div>



		<div id="myCarousel" class="carousel slide" data-ride="carousel">
		  <!-- Indicators -->
		  <ol class="carousel-indicators">
		  	  <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
			  <% (1..153).each do |i| %>
			  	<li data-target="#myCarousel" data-slide-to="#{@x}"></li>
			  	<% @x += 1 %>
			  <% end %>
			  <% @x = 2 %>
		  </ol>

		  <div class="carousel-inner">
		    <div class="item active">
		      	<%= image_tag image_path("weddings/#{@x}.jpg"), alt: "Carousel Image", class: "carousel-image" %>
		    </div>
		    
		     <% (1..153).each do |i| %>
		     	<div class="item">	
		     		<%= image_tag image_path("weddings/#{@x}.jpg"), alt: "Carousel Image", class: "carousel-image" %>
		     	</div>
		      	<% @x += 1 %>
		    <% end %>
		  </div>

		  <!-- Left and right controls -->
		  <a class="left carousel-control" href="#myCarousel" data-slide="prev">
		    <span class="glyphicon glyphicon-chevron-left"></span>
		    <span class="sr-only">Previous</span>
		  </a>
		  <a class="right carousel-control" href="#myCarousel" data-slide="next">
		    <span class="glyphicon glyphicon-chevron-right"></span>
		    <span class="sr-only">Next</span>
		  </a>

		</div>

	</div>
  </div>


  <div class="row footer-container gutter-max">
 	 <div class="col-sm-12 footer">
 	 	<p class="footer-text"> Email: SiennaTurecamoPhotography@gmail.com </p>
 	 	<p class="footer-text"> Phone: 540-660-9696 </p>
 	 	<br/>
 	 	<p class="footer-text"> All Creative content Â© Sienna Turecamo </p>
  	 </div>
  </div>

</div>